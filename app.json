{
  "expo": {
    "name": "QUIZZEC",
    "slug": "QUIZZEC",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/QUIZZEC.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "QUIZZEC",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#412E8B"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/QUIZZEC-Logo.png",
        "backgroundColor": "#412E8B"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.renan_soares_souza.QUIZZEC"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-barcode-scanner"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4f216c7a-3401-4aba-8732-4d1bfe16fe53"
      }
    },
    "owner": "renan_soares_souza"
  }
}
